{
  "name": "test-vercel-mcp",
  "version": "1.0.0",
  "description": "A mcpresso MCP server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target node",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "mcpresso": "^0.6.2",
    "zod": "^3.23.8",
    "hono": "^4.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "bun-types": "latest",
    "vercel": "^32.0.0"
  },
  "keywords": [
    "mcp",
    "mcpresso",
    "api",
    "server"
  ],
  "license": "MIT"
}