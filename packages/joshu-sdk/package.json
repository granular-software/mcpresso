{
  "name": "@joshu/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for communicating with JOSHU OS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "joshu",
    "os",
    "ai",
    "agent",
    "sdk"
  ],
  "author": "JOSHU Team",
  "license": "MIT",
  "dependencies": {
    "@automerge/automerge": "^2.2.9",
    "@joshu/os-types": "workspace:*",
    "ws": "^8.18.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 